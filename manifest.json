  "name": "Config_BS",
  "version": "1.0.0",

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },

  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "permissions": ["tabs"],
  "host_permissions": ["http://*/", "https://*/"]
}
